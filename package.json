{
  "name": "starion-sync",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": {
    "packages": [
      "./apps/**",
      "./libs/*"
    ]
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/starion-sync/main",
    "start:configurator:dev": "env-cmd -f ./apps/configurator/.env nest start @app/configurator --watch",
    "start:configurator:prod": "node dist/apps/configurator/main",
    "start:controller:dev": "env-cmd -f ./apps/controller/.env nest start @app/controller --watch",
    "start:controller:prod": "node dist/apps/controller/main",
    "start:executor:dev": "env-cmd -f ./apps/executor/.env nest start @app/executor --watch",
    "start:executor:prod": "node dist/apps/executor/main",
    "start:worker:dev": "env-cmd -f ./apps/worker/.env nest start @app/worker --watch",
    "start:worker:prod": "node dist/apps/worker/main",
    "start:cron-trigger:dev": "env-cmd -f ./apps/triggers/cron/.env nest start @app/trigger/cron --watch",
    "start:cron-trigger:prod": "node dist/apps/triggers/cron/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/starion-sync/test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/msal-node": "^1.17.3",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/microservices": "^9.4.2",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/platform-socket.io": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@temporalio/activity": "^1.7.4",
    "@temporalio/client": "^1.7.4",
    "@temporalio/common": "^1.7.4",
    "@temporalio/worker": "^1.7.4",
    "@temporalio/workflow": "^1.7.4",
    "aws-sdk": "^2.1083.0",
    "axios": "^0.26.0",
    "bcrypt": "^5.1.0",
    "bson-objectid": "^2.0.2",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "express-basic-auth": "^1.2.1",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "^6.2.4",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-paginate-v2": "^1.6.3",
    "mysql2": "^3.2.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/cache-manager": "^3.4.3",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@lib/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@lib/microservice(|/.*)$": "<rootDir>/libs/microservice/src/$1"
    }
  }
}