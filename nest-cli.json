{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/starion-sync/src",
  "compilerOptions": {
    "assets": [
      "**/*.json"
    ],
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/starion-sync/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/starion-sync",
  "projects": {
    "@app/starion-sync": {
      "type": "application",
      "root": "apps/starion-sync",
      "entryFile": "main",
      "sourceRoot": "apps/starion-sync/src",
      "compilerOptions": {
        "tsConfigPath": "apps/starion-sync/tsconfig.app.json"
      }
    },
    "@app/worker": {
      "type": "application",
      "root": "apps/worker",
      "entryFile": "main",
      "sourceRoot": "apps/worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/worker/tsconfig.app.json"
      }
    },
    "@lib/modules": {
      "type": "library",
      "root": "libs/modules",
      "entryFile": "index",
      "sourceRoot": "libs/modules/src",
      "compilerOptions": {
        "tsConfigPath": "libs/modules/tsconfig.lib.json"
      }
    },
    "@lib/core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "@app/controller": {
      "type": "application",
      "root": "apps/controller",
      "entryFile": "main",
      "sourceRoot": "apps/controller/src",
      "compilerOptions": {
        "tsConfigPath": "apps/controller/tsconfig.app.json"
      }
    },
    "@app/configurator": {
      "type": "application",
      "root": "apps/configurator",
      "entryFile": "main",
      "sourceRoot": "apps/configurator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/configurator/tsconfig.app.json"
      }
    }
  }
}